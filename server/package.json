{
  "name": "employee-erp-api",
  "version": "1.0.0",
  "description": "Employee ERP API",
  "main": "src/index.js",
  "scripts": {
    "dc-up": "docker-compose --env-file .env -f docker-compose.yml up -d",
    "dc-down": "docker-compose --env-file .env -f docker-compose.yml down",
    "dc-test-up": "docker-compose --env-file .env.test -f docker-compose.yml up -d",
    "dc-test-down": "docker-compose --env-file .env.test -f docker-compose.yml down",
    "db-migrate": "./node_modules/.bin/sequelize-cli db:migrate",
    "db-seed": "./node_modules/.bin/sequelize-cli db:seed:all",
    "db-reset": "npm run db-drop && npm run db-create && npm run db-migrate && npm run db-seed",
    "db-drop": "./node_modules/.bin/sequelize-cli db:drop",
    "db-create": "./node_modules/.bin/sequelize-cli db:create",
    "dev": "nodemon",
    "test": "NODE_ENV=test jest --detectOpenHandles --forceExit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^7.0.0",
    "@sentry/tracing": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.2",
    "connect-redis": "^6.1.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.0",
    "deepmerge": "^4.2.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.2",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "http-status-codes": "^2.2.0",
    "mysql2": "^2.3.3",
    "node-dependency-injection": "^2.7.3",
    "redis": "^4.1.0",
    "sequelize": "^6.17.0",
    "sequelize-cli": "^6.4.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "faker": "^5.5.3",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "supertest": "^6.2.2",
    "supertest-session": "^4.1.0"
  }
}
